<view class="flex-row tab-bar" >
    <view wx:for="{{tab_list}}" wx:key="index" class="tab-bar-item left-group_1 flex-col items-center view_1{{selected===index?'7':'8'}}" bindtap="switchTab" data-index="{{index}}" data-path="{{item.path}}">
      <image src="{{selected===index?item.selected_img:item.img}}" class="image_71" />
      <text decode="decode">{{item.name}}</text>
    </view>
  </view>